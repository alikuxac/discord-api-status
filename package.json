    "dotenv": "^14.3.0",
    "luxon": "^2.3.0",
    "mongoose": "^6.1.8",
    "quick.db": "^7.1.3"
  },
  "devDependencies": {
    "@types/luxon": "2.0.8",
    "@types/node": "16.11.62",
    "cross-env": "7.0.3",
    "rimraf": "3.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.5",
    "yarn": "1.22.19"
  },
  "git": {
    "type": "git",
    "url": "git+https://github.com/AliCuFe/discord-api-status.git"
