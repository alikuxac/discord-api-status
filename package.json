    "mongoose": "^6.1.8",
    "quick.db": "^9.0.0"
  },
  "devDependencies": {
    "@types/luxon": "3.2.0",
    "@types/node": "18.19.26",
    "cross-env": "7.0.3",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "yarn": "1.22.19"
  },
  "git": {
    "type": "git",
